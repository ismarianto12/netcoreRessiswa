@model YourReportModel

<h2>@Model.Title</h2>
<p>Tanggal: @Model.Date.ToString("dd MMM yyyy")</p>

<table border="1" cellpadding="5">
    <thead>
        <tr><th>Nama</th><th>Harga</th></tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Items)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Price.ToString("C")</td>
            </tr>
        }
    </tbody>
</table>
